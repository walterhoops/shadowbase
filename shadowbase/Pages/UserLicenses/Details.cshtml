@page "{id:int?}"
@model shadowbase.Pages.UserLicenses.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>LicenseData</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LicenseData.UserID)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LicenseData.UserID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LicenseData.reLicense)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LicenseData.reLicense)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LicenseData.mbLicense)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LicenseData.mbLicense)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LicenseData.hiLicense)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LicenseData.hiLicense)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LicenseData.UserData)
        </dt>
        <dd class="col-sm-10">
            <table class="table">
                <tr>
                    <th>UserId</th>
                    
                </tr>
                @foreach (var item in Model.LicenseData.UserData)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Id)
                        </td>
                        
                    </tr>
                }
            </table>
        </dd>
    </dl>
